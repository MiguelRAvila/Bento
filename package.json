{
  "name": "bento",
  "version": "1.2.5",
  "description": "üç± Minimalist, elegant and hackable startpage inspired by the Bento box!",
  "author": "Andrej Borisenko, Miguel √Åvila",
  "license": "ISC",
  "scripts": {
    "clean": "npm run clean:dist && npm run clean:cache",
    "clean:dist": "exec rm -rf ./dist/*",
    "clean:cache": "exec rm -rf ./.parcel-cache/*",
    "dev": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --public-url ./",
    "build-dev": "parcel build ./src/index.html --no-optimize --public-url ./"
  },
  "source": "src/index.html",
  "staticFiles": {
    "staticPath": "assets",
    "staticOutPath": "assets"
  },
  "dependencies": {
    "@parcel/service-worker": "^2.10.3",
    "chart.js": "^4.4.0",
    "lucide-vue-next": "^0.294.0",
    "vue": "^3.3.9",
    "vue-chartjs": "^5.2.0"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.10.3",
    "parcel": "^2.10.3",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss-dark-theme-class": "^1.1.0"
  },
  "optionalDependencies": {
    "@parcel/watcher-darwin-x64": "^2.3.0"
  }
}