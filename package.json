{
  "name": "bento",
  "version": "1.2.5",
  "description": "üç± Minimalist, elegant and hackable startpage inspired by the Bento box!",
  "author": "Andrej Borisenko, Miguel √Åvila",
  "license": "ISC",
  "scripts": {
    "clean": "exec rm -rf ./dist/* && sleep 1",
    "dev": "npm run clean && parcel ./src/index.html",
    "build": "npm run clean && parcel build ./src/index.html --public-url ./",
    "build-dev": "npm run clean && parcel build ./src/index.html --no-optimize --public-url ./",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose",
    "publish": "npm run build && npm run push-gh-pages",
    "publish-dev": "npm run build-dev && npm run push-gh-pages"
  },
  "source": "src/index.html",
  "staticFiles": {
    "staticPath": "assets",
    "staticOutPath": "assets"
  },
  "dependencies": {
    "lucide-vue-next": "^0.288.0",
    "vue": "^3.3.6"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.10.0",
    "parcel": "latest",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss-dark-theme-class": "^1.0.0",
    "push-dir": "^0.4.1"
  }
}
