{
  "name": "bento",
  "version": "1.2.5",
  "description": "üç± Minimalist, elegant and hackable startpage inspired by the Bento box!",
  "author": "Andrej Borisenko, Miguel √Åvila",
  "license": "ISC",
  "scripts": {
    "clean": "exec rm -rf ./dist/* && exec rm -rf ./.parcel-cache/*",
    "dev": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --public-url ./",
    "build-dev": "parcel build ./src/index.html --no-optimize --public-url ./",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose",
    "publish": "npm run build && npm run push-gh-pages",
    "publish-dev": "npm run build-dev && npm run push-gh-pages"
  },
  "source": "src/index.html",
  "staticFiles": {
    "staticPath": "assets",
    "staticOutPath": "assets"
  },
  "dependencies": {
    "@parcel/service-worker": "^2.10.2",
    "chart.js": "^4.4.0",
    "lucide-vue-next": "^0.291.0",
    "vue": "^3.3.7",
    "vue-chartjs": "^5.2.0"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.10.2",
    "parcel": "^2.10.2",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss-dark-theme-class": "^1.0.0",
    "push-dir": "^0.4.1"
  },
  "optionalDependencies": {
    "@parcel/watcher-darwin-x64": "^2.3.0"
  }
}
